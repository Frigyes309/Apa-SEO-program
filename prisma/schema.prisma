model LinkedPage {
  id   Int    @id @default(autoincrement())
  link String
}

model ReferedPageMain {
  id      Int    @id @default(autoincrement())
  refpref String
}

model Domain {
  dr        Int
  raw       String @unique
  protocol  Int //0-4, 0: https://www., 1: http://www., 2: https://, 3: http://
  refPrefId Int
  lpId      Int
}
